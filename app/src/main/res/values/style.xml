    <style name="TextField">
        <item name="android:background">@drawable/bg_textfield_surface_8</item>
        <item name="android:textAppearance">@style/BodyLarge</item>
        <item name="android:padding">18dp</item>
    </style>

    <style name="ButtonPrimary">
        <item name="android:textAppearance">@style/TitleMedium.SemiBold</item>
        <item name="android:padding">16dp</item>
        <item name="cornerRadius">8dp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="ButtonPrimary.Square" parent="ButtonPrimary">
        <item name="android:textAppearance">@style/TitleMedium.SemiBold</item>
        <item name="android:padding">16dp</item>
        <item name="cornerRadius">0dp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="ButtonClaim">
        <item name="android:backgroundTint">?attr/colorSecondary</item>
        <item name="android:textAppearance">@style/LabelSmall.White</item>
        <item name="android:paddingVertical">12dp</item>
        <item name="android:paddingHorizontal">22dp</item>
        <item name="cornerRadius">16dp</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="ShapeAppearance8">
        <item name="cornerSize">8dp</item>
    </style>
    <style name="ShapeAppearance16">
        <item name="cornerSize">16dp</item>
    </style>
    <style name="ShapeAppearanceOverlayBottom16">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomRight">16dp</item>
        <item name="cornerSizeBottomLeft">16dp</item>
    </style>
    <style name="ShapeAppearanceOverlayTop16">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeTopRight">16dp</item>
    </style>
    <style name="ShapeAppearanceOverlayCircle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50dp</item>
    </style>
